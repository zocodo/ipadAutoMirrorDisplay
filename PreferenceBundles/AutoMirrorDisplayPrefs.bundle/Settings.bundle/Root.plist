<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>PreferenceSpecifiers</key>
    <array>
        <!-- 分组标题 -->
        <dict>
            <key>Type</key>
            <string>PSGroupSpecifier</string>
            <key>Title</key>
            <string>AutoMirrorDisplay 设置</string>
        </dict>
        <!-- 镜像/扩展 单选 -->
        <dict>
            <key>Type</key>
            <string>PSRadioGroupSpecifier</string>
            <key>Title</key>
            <string>显示模式</string>
            <key>Key</key>
            <string>mirrorMode</string>
            <key>DefaultValue</key>
            <string>YES</string>
            <key>Titles</key>
            <array>
                <string>镜像</string>
                <string>扩展</string>
            </array>
            <key>Values</key>
            <array>
                <string>YES</string>
                <string>NO</string>
            </array>
			<!-- 改动后发通知 -->
            <key>PostNotification</key>
            <string>com.zocodo.automirrordisplay.prefs.changed</string>
        </dict>
        <!-- 日志开关 -->
        <dict>
            <key>Type</key>
            <string>PSSwitchSpecifier</string>
            <key>Title</key>
            <string>启用调试日志</string>
            <key>Key</key>
            <string>logEnabled</string>
            <key>DefaultValue</key>
            <false/>
			<!-- 改动后发通知 -->
			<key>PostNotification</key>
            <string>com.zocodo.automirrordisplay.prefs.changed</string>
        </dict>
    </array>
</dict>
</plist>
